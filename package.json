{
  "name": "vscode-nupkg",
  "displayName": "NuGet NuPkg Viewer",
  "description": "Display all information from a NuPkg file",
  "version": "0.0.5",
  "publisher": "eridem",
  "keywords": [
    "nuget",
    "nupkg",
    "viewer",
    "manifest",
    "unzip"
  ],
  "author": {
    "name": "Miguel Ángel Domínguez Coloma",
    "email": "m@eridem.net",
    "url": "http://eridem.net"
  },
  "markdown": "github",
  "contributors": [],
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "http://eridem.net/nuget-nupkg-viewer-visual-studio-code-extension/",
  "icon": "icon.svg",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "bugs": {
    "url": "https://github.com/eridem/vscode-nupkg/issues",
    "email": "m@eridem.net"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eridem/vscode-nupkg"
  },
  "badges": [
    {
      "url": "https://david-dm.org/eridem/vscode-nupkg/dev-status.svg",
      "href": "https://david-dm.org/eridem/vscode-nupkg",
      "description": "Dependency Status"
    }
  ],
  "activationEvents": [
    "onCommand:extension.preview"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "languages": [
      {
        "id": "nupkg",
        "aliases": [
          "NuPkg",
          "nupkg"
        ],
        "extensions": [
          ".nupkg"
        ]
      }
    ],
    "commands": [
      {
        "command": "extension.preview",
        "title": "Preview NuPkg contents",
        "icon": {
          "light": "./images/menu-light.svg",
          "dark": "./images/menu-dark.svg"
        },
        "category": "NuPkg"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "extension.preview",
          "alt": "extension.preview",
          "group": "navigation",
          "when": "resourceLangId == nupkg"
        }
      ],
      "explorer/context": [
        {
          "command": "extension.preview",
          "alt": "extension.preview",
          "group": "navigation",
          "when": "resourceLangId == nupkg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "@types/node": "^6.0.40"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "js-yaml": "^3.6.1",
    "shelljs": "^0.7.5",
    "smart-nupkg-metadata-reader": "^1.0.0"
  }
}